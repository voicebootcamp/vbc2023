    <field name="userid" filter="int" min="1" type="number" readonly="true" label="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_USERID" description="COM_LIMITACTIVELOGINS_FORM_DESC_LOG_USERID" hint="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_USERID"/>
    <field name="username" filter="safehtml" type="text" readonly="true" label="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_USERNAME" description="COM_LIMITACTIVELOGINS_FORM_DESC_LOG_USERNAME" hint="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_USERNAME"/>
