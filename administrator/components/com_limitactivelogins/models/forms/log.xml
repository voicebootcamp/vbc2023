      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="created_by" type="createdby" default="0" label="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_CREATED_BY" description="COM_LIMITACTIVELOGINS_FORM_DESC_LOG_CREATED_BY" hidden="true" hint="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_CREATED_BY"/>
    <field name="modified_by" type="modifiedby" default="0" label="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_MODIFIED_BY" description="COM_LIMITACTIVELOGINS_FORM_DESC_LOG_MODIFIED_BY" hidden="true" hint="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_MODIFIED_BY"/>
    <field name="session_id" filter="safehtml" type="text" required="true" readonly="true" label="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_SESSION_ID" description="COM_LIMITACTIVELOGINS_FORM_DESC_LOG_SESSION_ID" hint="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_SESSION_ID"/>
    <field name="user_agent" filter="safehtml" type="text" readonly="true" label="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_USER_AGENT" description="COM_LIMITACTIVELOGINS_FORM_DESC_LOG_USER_AGENT" hint="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_USER_AGENT"/>
    <field name="country" filter="safehtml" type="text" readonly="true" label="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_COUNTRY" description="COM_LIMITACTIVELOGINS_FORM_DESC_LOG_COUNTRY" hint="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_COUNTRY"/>
    <field name="browser" filter="safehtml" type="text" readonly="true" label="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_BROWSER" description="COM_LIMITACTIVELOGINS_FORM_DESC_LOG_BROWSER" hint="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_BROWSER"/>
    <field name="operating_system" filter="safehtml" type="text" readonly="true" label="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_OPERATING_SYSTEM" description="COM_LIMITACTIVELOGINS_FORM_DESC_LOG_OPERATING_SYSTEM" hint="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_OPERATING_SYSTEM"/>
    <field name="ip_address" filter="safehtml" type="text" readonly="true" label="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_IP_ADDRESS" description="COM_LIMITACTIVELOGINS_FORM_DESC_LOG_IP_ADDRESS" hint="COM_LIMITACTIVELOGINS_FORM_LBL_LOG_IP_ADDRESS"/>
